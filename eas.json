{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "env": {
          "NODE_ENV": "development",
          "EXPO_NO_CAPABILITY_SYNC": "1",
          "EXPO_NO_GIT_STATUS": "1"
        }
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "NODE_ENV": "production",
          "EXPO_NO_CAPABILITY_SYNC": "1",
          "EXPO_NO_GIT_STATUS": "1"
        }
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "local",
        "cache": {
          "disabled": false
        },
        "env": {
          "NODE_ENV": "production",
          "EXPO_ENV": "production",
          "EXPO_NO_CAPABILITY_SYNC": "1",
          "EXPO_NO_GIT_STATUS": "1"
        }
      }
    }
  }
}
