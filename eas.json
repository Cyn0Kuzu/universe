{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "default"
      },
      "android": {
        "resourceClass": "default",
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "default",
        "simulator": true
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "default"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": "buildNumber",
        "image": "latest"
      },
      "android": {
        "buildType": "app-bundle",
        "resourceClass": "large",
        "autoIncrement": "versionCode"
      },
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "cayankuzu.0@gmail.com",
        "ascAppId": "6754333896",
        "appleTeamId": "HBRG8P523Z"
      },
      "android": {
        "serviceAccountKeyPath": "./android-service-account.json",
        "track": "production"
      }
    }
  }
}
