{
  "cli": {
    "version": ">= 10.0.0"
  },
  "build": {
    "development": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "env": {
          "EXPO_NO_CAPABILITY_SYNC": "1",
          "EXPO_NO_GIT_STATUS": "1"
        }
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_NO_CAPABILITY_SYNC": "1",
          "EXPO_NO_GIT_STATUS": "1"
        }
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_NO_CAPABILITY_SYNC": "1",
          "EXPO_NO_GIT_STATUS": "1"
        }
      }
    }
  }
}
